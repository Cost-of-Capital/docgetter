{
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "api/index.py"
      }
    ],
    "functions": {
      "app/api/*": {
        "maxDuration": 60 
      },
      "app/modules/*": {
        "maxDuration": 60 
      }
    }
  }